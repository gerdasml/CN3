        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CN3"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="inputBox" HorizontalAlignment="Left" Height="63" Margin="10,246,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="497" KeyDown="inputBox_KeyDown"/>
        <TextBox x:Name="tablesBox" HorizontalAlignment="Left" IsReadOnly="True" Height="231" Margin="10,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="204"/>
        <TextBox x:Name="answersBox" HorizontalAlignment="Left" IsReadOnly="True" Height="231" Margin="219,10,0,0" TextWrapping="Wrap"  VerticalAlignment="Top" Text="> " Width="288"/>

    </Grid>
</Window>
